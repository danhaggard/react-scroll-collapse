{
  "name": "react-scroll-collapse",
  "private": true,
  "version": "0.1.0",
  "description": "React/redux wrappers for auto-scrolling react-collapse components",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "ghPages": "NODE_ENV=ghPages webpack",
    "dist": "npm run clean && npm run copy && webpack --progress --bail --env dist -p",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release:major": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
    "release:minor": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
    "release:patch": "npm version prerelease && git push --follow-tags && npm publish --tag beta",
    "serve:ex": "webpack-dev-server --open --env ex",
    "serve:dev": "NODE_ENV=dev webpack-dev-server --open",
    "serve:dist": "webpack-dev-server --open --env dist -p --progress",
    "serve:ghPages": "NODE_ENV=ghPages webpack-dev-server --open -p --progress",
    "start": "npm run serve:dev",
    "test": "NODE_ENV=test karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false --reporters=mocha,coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danhaggard/react-scroll-collapse"
  },
  "bugs": "https://github.com/danhaggard/react-scroll-collapse/issues",
  "keywords": [],
  "author": "Dan Haggard",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^1.0.0",
    "eslint-plugin-react": "^5.0.0",
    "expect": "^1.20.2",
    "file-loader": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-junit-reporter": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.0.1",
    "react-collapse": "^2.3.3",
    "react-dom": "^15.0.1",
    "react-height": "^2.1.1",
    "react-motion": "^0.4.5",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-saga": "^0.12.0",
    "reselect": "^2.5.4"
  }
}
