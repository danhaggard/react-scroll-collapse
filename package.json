{
  "name": "react-scroll-collapse",
  "version": "2.0.0",
  "description": "React / redux component wrappers for auto-scrolling react-collapse components",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "gh-pages": "git checkout gh-pages && git rebase origin/master --force-rebase && npm run build:ghPages && git add . && git commit --amend --no-edit && git push --force && git checkout master",
    "build:ghPages": "cross-env NODE_ENV=ghPages webpack --progress",
    "dist": "npm run clean && cross-env NODE_ENV=production webpack --progress",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "prepublish": "npm run dist",
    "release:major": "npm version major && git push --follow-tags && npm publish --tag latest",
    "release:minor": "npm version minor && git push --follow-tags && npm publish --tag latest",
    "release:patch": "npm version patch && git push --follow-tags && npm publish --tag latest",
    "serve:dev": "cross-env NODE_ENV=development node server/dev-server.js",
    "serve:ghPages": "cross-env NODE_ENV=ghPages node server/dev-server.js",
    "serve:hot": "cross-env NODE_ENV=hot node server/dev-server-hot.js",
    "start": "npm run serve:dev",
    "stats": "cross-env NODE_ENV=production webpack --profile --json > ./stats/stats.json",
    "stats-gh": "cross-env NODE_ENV=ghPages webpack --profile --json > ./stats/stats-gh.json",
    "test": "jest --runInBand --config ./test/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danhaggard/react-scroll-collapse"
  },
  "bugs": "https://github.com/danhaggard/react-scroll-collapse/issues",
  "keywords": [
    "react",
    "redux",
    "react-motion",
    "react-collapse",
    "component-wrapper",
    "scroll",
    "auto-scroll"
  ],
  "author": "Dan Haggard",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.21.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@welldone-software/why-did-you-render": "^3.6.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-preset-minify": "^0.5.2",
    "classnames": "^2.3.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^3.0.1",
    "humps": "^2.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "lodash.isequal": "^4.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "normalize.css": "^8.0.1",
    "opener": "^1.5.2",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "react-collapse": "^4.0.3",
    "react-height": "^3.0.2",
    "react-hot-loader": "^4.13.1",
    "react-pure-render": "^1.0.2",
    "react-router-dom": "^5.3.4",
    "react-test-renderer": "^16.14.0",
    "redux": "^4.2.1",
    "redux-devtools": "^3.7.0",
    "redux-devtools-dock-monitor": "^1.2.0",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.2",
    "rimraf": "^2.7.1",
    "sass": "^1.62.0",
    "sass-loader": "^10.4.1",
    "style-loader": "^0.19.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.13.2",
    "worker-loader": "^3.0.8"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-collapse": "^4.0.3",
    "react-dom": "^16.8.6",
    "react-height": "^3.0.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-motion": "^0.5.2",
    "react-redux": "^7.0.2"
  }
}
